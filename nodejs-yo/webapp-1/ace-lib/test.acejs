<style>
	div.app-content-wrapper {
		margin-top:0;
	}
</style>
<div class="app-content-wrapper">
<div class="app-content">
<h1>Manggis Schedular</h1>
<div class="row section">
	<div class="col-md-3 col-sm-5">
			<select id="search-group" class="form-control responsive" tabindex="1" value="<%=groupId%>" onchange="G.pageRequire('./job-list').changeGroup(this.value);">
				<option data-rfl-click="changeGroup" value="">All Groups</option>
				<%for(var i = 0; i < groups.length; i++) {
				%>
					<option  value="<%=i%>" <%=i == groupId ? 'selected="selected"' : ''%>><%=groups[i]%></option>
				<%}%>
			</select> 
	</div>
	<div class="col-md-3 col-sm-1">
		
	</div>
	<div class="col-md-6 col-sm-6 text-right">		
			<a href="javascript:void(0);" data-rfl-click="createJob" class="btn btn-primary responsive">Create</a>
	</div>
</div>